{
  "name": "react-input-textarea",
  "version": "1.0.2",
  "description": "Have your React input and text area be styled, right down to the placeholder. Have them be styled inlined, without any external CSS.",
  "author": "Enigma Technologies <contact@enigma.io> (http://enigma.io)",
  "main": "index.js",
  "scripts": {
    "preview": "webpack-dev-server -d --content-base ./public",
    "bundle": "babel --stage 0 src/index.js > build/index.js && babel --stage 0 src/helpers.js > build/helpers.js",
    "test": "mocha --require babelhook"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/shovon/react-input-textarea.git"
  },
  "keywords": [
    "placeholder",
