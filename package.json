    "react",
    "react-component"
  ],
  "contributors": [
    "enigma.io",
    "Joe Natalzia",
    "Evan Jacobs"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shovon/react-input-textarea/issues"
  },
  "devDependencies": {
    "babel": "^5.5.8",
    "babel-core": "^5.5.8",
    "babel-loader": "^5.1.4",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  },
  "peerDependencies": {
    "react": "^0.13.3"
  },
  "dependencies": {
    "object-assign": "^3.0.0"
